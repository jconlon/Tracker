  <OCD description="A MongoDB client provider." id="com.verticon.mongo" name="MongoDB Client Provider">
    <AD default="No wires" description="This optional attribute has no functionality other than to visualize and document this service in the iRouter Monitor." id="tracker.wiring.group.name" name="Wiring group name" required="false"/>
    <AD default="MongoDB on Local Host" description="Phsical Location of the MongoDB Server." id="external.location" name="Location" required="false"/>
    <AD default="Description of device or service." description="Description of the MongoDB Service." id="external.description" name="Description" required="false"/>
    <AD default="localhost:27017" description="Semicolon separated list of MongoDB Server names and ports. Example tuna:27001;tuna:27002;tuna:27003" id="external.connection.uri" name="MongoDB Server" required="true"/>
    <AD default="tracker" description="Name of the MongoDB database to use." id="dbname" name="MongoDB Database Name" required="true"/>
    <AD description="Optional name of the MongoDB User." id="username" name="MongoDB User Name" required="false"/>
    <AD description="Password for the MongoDB user." id="password" name="MongoDB User Password" required="false"/>
    <AD default="gateway" description="The type of external device or service. This optional attribute has no functionality other than to visualize and document this service in the iRouter Monitor." id="external.service.type" name="Type" required="true">
      <Option label="Gateway Service" value="gateway"/>
    </AD>
    <AD default="MongoDB" description="Name to label the node in the iRouter Monitor View graphics. This optional attribute has no functionality other than to visualize and document this service in the iRouter Monitor." id="tracker.monitor.label" name="Component container label"/>
  </OCD>
  <OCD description="Gateway for producing a list of animal life data records extracted from a specified Premises model. Fields within each record can be ordered, values can be transformed, and static values can be added." id="com.verticon.tracker.irouter.animal.data" name="Animal Life Data Gateway">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD default="event://com/verticon/tracker/reader" description="Identifier to associate this service with a premises data model." id="connection.uri" name="Connection uri" required="true"/>
    <AD cardinality="15" description="Selected attributes of the animal premises model to include as record fields. If none are configured, all attributes will be extraced." id="animal.life.data.fields" name="Animal life data fields" required="false">
      <Option label="Electronic ID tag" value="id"/>
      <Option label="Sex" value="sex"/>
      <Option label="Sex Code" value="sexCode"/>
      <Option label="Species" value="species"/>
      <Option label="Species Code" value="speciesCode"/>
      <Option label="Visual ID tag" value="visualID"/>
      <Option label="Type of animal" value="type"/>
      <Option label="Alternate ID" value="alternativeID"/>
      <Option label="Breed" value="breed"/>
      <Option label="Last recorded weight" value="weight"/>
      <Option label="Last calculated weight gain" value="weightGainPerDay"/>
      <Option label="Time of last recorded event" value="lastEventDateTime"/>
      <Option label="Date of Birth" value="birthDate"/>
      <Option label="Current age in days" value="ageInDays"/>
      <Option label="Comments" value="comments"/>
    </AD>
    <AD cardinality="100" description="Optional transformer of animal attribute values to output field valus. Each entry contains an attribute name, a value to map, and an output value each separated by a | character. To add static values to each record use the attribute name STATIC, a integer position, and the static value. For example STATIC|3|HelloWorld will add the text HelloWorld to position 3 in the record. " id="animal.life.data.mapping" name="Data value transformer" required="false"/>
    <AD cardinality="1" description="The scope to send the extracted animal life data records. (Use full scope names)" id="wireadmin.producer.scope" name="Producer scope"/>
    <AD default="60" description="Time to wait in seconds before retrieving new data from the premises model." id="refresh.seconds" name="Refresh timer" required="false" type="Integer"/>
    <AD default="Animal Data Gateway" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Monitor node label" required="false"/>
  </OCD>
  <OCD description="Business logic controller for integrating TruTest scales and Mettler balances for measuring animal blood weights as a percentage of total animal weight." id="com.verticon.tracker.irouter.bwcontrol" name="Blood Weight Controller">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD default="1" description="Unit of measurement for blood weights." id="trutest.field.units" name="Units for displaying blood weights" type="Integer">
      <Option label="grams" value="0"/>
      <Option label="kilograms" value="1"/>
    </AD>
    <AD default="{SABP500}%13%" description="TruTest command to use for turn on the audible alarm. The integer in the command determines the time duration of the audible." id="trutest.beepCommand" name="Alarm command" required="false"/>
    <AD default="5" description="Time in seconds between audible alarms." id="abbott.bwalarm.period" name="Alarm interval in seconds" required="false" type="Long"/>
    <AD default=".01" description="Ratio of actual animal weight to calculated blood weight." id="abbott.bwmult" name="BW multiplier" required="false" type="Double"/>
    <AD default=".75" description="Ratio of actual blood weight to calculated blood weight that triggers alarming." id="abbott.bwalarm.trigger" name="Animal to BW percent trigger for alarm" required="false" type="Double"/>
    <AD default="5" description="The weight of animal in kilograms that will trigger the calculating of blood weights.    This value prevents negative scale weights from display erroneous values." id="abbott.calbw.threashold" name="Animal weight calculation threashold" required="false" type="Long"/>
    <AD default="{SXD19}%13%%10%{SXC16}%13%" description="TruTest command that identifies the field for displaying Actual BW" id="trutest.actbw.field" name="Actual BW field" required="false"/>
    <AD default="{SXD18}%13%%10%{SXC16}%13%" description="TruTest command that identifies the field for displaying Calc BW" id="trutest.calbw.field" name="Calc BW field" required="false"/>
    <AD default="true" description="If true a record key press will stop the production of ActBW and CalcBW.  Calculation will resume at zeroing of animal weight." id="abbott.stop.production.after.record" name="Stop production after Record" required="false" type="Boolean"/>
    <AD cardinality="3" default="animal.weight,mettler.weight,transaction.state" description="The consumable scopes carrying the animal weight, blood weight, and transaction state. (Use full scope names)" id="wireadmin.consumer.scope" name="Consumable scope" required="true"/>
    <AD default="animal.weight" description="The consumable scopes (from the list of consumable scopes) carrying the animal weight. (Use full scope names)" id="animal.weight" name="Animal Weight scope" required="true"/>
    <AD default="mettler.weight" description="The consumable scope (from the list of consumable scopes) carrying the blood weight. (Use full scope names)" id="blood.weight" name="Blood Weight scope" required="true"/>
    <AD cardinality="1" default="trutest.commands" description="The producer scope to send the adapted TruTest commands. (Use full scope names)" id="wireadmin.producer.scope" name="Producer scope" required="true"/>
    <AD default="BWControl" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Monitor node label" required="false"/>
  </OCD>
  <OCD description="Gateway between Tracker Premises RFID readers and iRouter. Picks up tag id reads from configured readers and produces iRouter animal ids." id="com.verticon.tracker.irouter.eid.event" name="Premises EID Reader Gateway">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD default="com/verticon/tracker/reader" description="Default event topics. Do not change." id="event.topics" name="Event Topics"/>
    <AD default="(com.verticon.tracker.reader.name=*)" description="Event filter to restrict reception of reads from certain Tracker RFID readers. Example: (com.verticon.tracker.reader.name=*:My Reader Name)" id="event.filter" name="Reader Filter" required="true"/>
    <AD cardinality="1" default="reader.eid" description="The produced scope containing the animal EIDs. (Use full scope names)" id="wireadmin.producer.scope" name="Producer scope" required="true"/>
    <AD default="event://com/verticon/tracker/reader" description="Identifier to associate this service with an external node in the monitor view." id="connection.uri" name="Connection uri" required="true"/>
    <AD default="EID Gateway" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Monitor node label" required="false"/>
  </OCD>
  <OCD description="Gateway between OSGi EventAdmin messaage bus and iRouter. Initally only forwards String products." id="com.verticon.tracker.irouter.eventadmin.gateway" name="EventAdmin Gateway">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD default="com/verticon/tracker/irouter" description="Default event topics. Do not change." id="event.topics" name="Event Topics"/>
    <AD cardinality="1" default="event.admin.gateway" description="The produced scope containing the products. (Use full scope names)" id="wireadmin.producer.scope" name="Producer scope" required="true"/>
    <AD default="EventAdmin Gateway" description="Name to label node in the iRouter Monitor View graphics and other user interface components." id="tracker.monitor.label" name="Component container label" required="false"/>
  </OCD>
  <OCD description="Controls passing or blocking of objects based on the detection of a specific State. Only non-State objects are passed." id="com.verticon.tracker.irouter.flow.terminator" name="Flow Terminator">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD cardinality="5" description="Names of the consumed scopes containing information and triggering State objects.  Only the information (non State traffic) will be passed through when flow is unterminated. (Use full scope names)" id="wireadmin.consumer.scope" name="Consumed Information and Control scopes"/>
    <AD description="The name of the state that terminates information flow." id="terminator.state.name" name="Flow Terminator State Name" required="true"/>
    <AD description="The value of the state that terminates information flow." id="terminator.state.value" name="Flow Terminator State Value" required="true" type="Integer"/>
    <AD cardinality="4" description="Name of the produced scopes containing information passed through from the information consumbable scopes. (Use full scope names)" id="wireadmin.producer.scope" name="Information Producer scope" required="true"/>
    <AD cardinality="4" description="Consumer scope to producer scope map. Use full scope names with a colon as the delimiter between the scopes.  Example in.scope:out.scope" id="terminator.scope.map" name="Scope Map"/>
    <AD default="Terminator" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Component container label"/>
  </OCD>
  <OCD description="Compares two measurements. Produces a state with a negative one, zero, or a positive one if the first measurement is less than, equal to, or greater than the second measurement." id="com.verticon.tracker.irouter.measurement.comparator" name="Measurement Comparator">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD cardinality="2" description="The two input measurements to compare. (Use full scope names)" id="wireadmin.consumer.scope" name="Consumable scopes" required="true"/>
    <AD cardinality="1" description="The result product output state. (Use full scope name)" id="wireadmin.producer.scope" name="Producer scope" required="true"/>
    <AD default="Comparator" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Component container label"/>
  </OCD>
  <OCD description="Converts input measurements based on a mathmatical operation." id="com.verticon.tracker.irouter.measurement.converter" name="Measurement Converter">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD cardinality="1" description="The input measurement scope to transform. (Use full scope names)" id="wireadmin.consumer.scope" name="Consumable scope" required="true"/>
    <AD cardinality="1" description="The transformed product output measurement scope. (Use full scope names)" id="wireadmin.producer.scope" name="Producer scope" required="true"/>
    <AD default="add" id="operation.type" name="Operation on measurement">
      <Option label="add" value="add"/>
      <Option label="subtract" value="subtract"/>
      <Option label="multiply" value="multiply"/>
      <Option label="divide" value="divide"/>
    </AD>
    <AD id="operation.value" name="Value to use for operation" required="true" type="Double"/>
    <AD default="Converter" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Component container label" required="false"/>
  </OCD>
  <OCD description="Gateway for sending measurements, positions and animal tag ids to Tracker transaction editors as animal history events." id="com.verticon.tracker.irouter.measurement.event" name="Premises Measurement Event Gateway">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD cardinality="5" default="animal.weight,animal.eid,transaction.state" description="Comma delimited list of the consumed scopes. Required scopes: measurement and the transaction ending triggering state. A scope for animal tag id is optional and used for information that has not previously been associated with an identification. (Use full scope names)." id="wireadmin.consumer.scope" name="Consumable scopes" required="true"/>
    <AD default="false" description="If True sends all cached measurements or positions when an eid is received. All received states are ignored when true. When false use the state information for triggering." id="triggeron.eid" name="Trigger on EID" required="true" type="Boolean"/>
    <AD default="false" description="If True sends only the last cached measurement or position recieved. When false all cached positions and measurements are sent." id="onlysend.last.envelope" name="Send only the last value" required="true" type="Boolean"/>
    <AD default="transaction.state" description="The name of the state indicating that measurement and the animal id should be forwarded as an animal history event." id="consumer.transaction.state" name="Name of the Triggering State" required="true"/>
    <AD default="1" description="The value of the state indicating that measurement and the animal id should be forwarded as an animal history event." id="consumer.transaction.state.value" name="Value of the Triggering State" required="true" type="Integer"/>
    <AD default="event://com/verticon/tracker/reader" description="Identifier to associate this service with an external node in the monitor view." id="connection.uri" name="Connection uri" required="true"/>
    <AD default="Premises Measurement" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Monitor node label" required="false"/>
  </OCD>
  <OCD description="Gateway for caching consumed measurements and aniamal tag ids, and saving them at triggered transaction boundries as log entries identified with animal IDs." id="com.verticon.tracker.irouter.measurement.logger" name="Measurement Log Gateway">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD cardinality="5" default="animal.weight,animal.eid,transaction.state" description="Comma delimited list of the scopes for measurements, animal tag ids, and trigger states. (Use full scope names)" id="wireadmin.consumer.scope" name="Consumable scopes" required="true"/>
    <AD default="transaction.state" description="The name of the state indicating that transaction should be logged." id="consumer.transaction.state" name="Name of the Control State" required="true"/>
    <AD default="1" description="The value of the state indicating that transaction should be logged." id="consumer.transaction.state.value" name="Value of the Control State" required="true" type="Integer"/>
    <AD default="0" description="Kind of log entry to save the cached transaction. Normalized log entries are comma delimited and include all measurements. Aggregated log entries are separate log entries per measurement." id="logger.type" name="Type of logger" required="true" type="Integer">
      <Option label="Normalized" value="0"/>
      <Option label="Aggegated" value="1"/>
    </AD>
    <AD default="transaction" description="Name of the file to send logs." id="logger.name" name="Logger name" required="false"/>
    <AD default="Logger" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Monitor node label" required="false"/>
    <AD default="file:///~/transaction.log" description="Identifier to associate this service with an external node in the monitor view." id="connection.uri" name="Connection uri" required="false"/>
  </OCD>
  <OCD description="Inspect measurements for conditions and produce a state with a value of 2, if the conditions are met." id="com.verticon.tracker.irouter.measurement.trigger" name="Measurement Trigger">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD cardinality="1" description="Names of the measurement scope to inspect. (Use full scope names)" id="wireadmin.consumer.scope" name="Consumable scope" required="true"/>
    <AD description="Value of the low measurement threashold." id="value.low" name="Low Measurement Threshold" required="true" type="Double"/>
    <AD description="Value of the high measurement threshold." id="value.high" name="High Measurement Threshold" required="true" type="Double"/>
    <AD default="raising" description="Type of threashold that will fire the trigger." id="operation.type" name="Operation on measurement">
      <Option label="Trigger on raising threshold" value="rising"/>
      <Option label="Trigger on falling threshold" value="falling"/>
    </AD>
    <AD cardinality="1" description="The scope containing triggered states. (Use full scope names)" id="wireadmin.producer.scope" name="Producer scope" required="true"/>
    <AD default="Measurement Trigger" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Component container label"/>
  </OCD>
  <OCD description="Gateway for consumming measurements and saving them as values in files." id="com.verticon.tracker.irouter.measurement.trigger.file" name="Measurement Trigger File">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD cardinality="5" default="ohaus.weight,event.admin.gateway" description="Comma delimited list of the scopes for measurements and strings. Only a measurement scope is required. A string will be used to reconfigure this component." id="wireadmin.consumer.scope" name="Consumable scopes" required="true"/>
    <AD default="measurement-***.txt" description="Name of file to use for output. Asterixs will be replaced by a timestamp." id="file.name.pattern" name="File Name Pattern" required="true"/>
    <AD default="POUNDS" description="Unit of measurement for outputing weights. (All other measurements types will be output without conversion.)" id="weight.measurement.units" name="Units for outputing weight measurement values.">
      <Option label="Pounds" value="POUNDS"/>
      <Option label="Ounces" value="OUNCES"/>
      <Option label="Kilograms" value="KILOGRAMS"/>
      <Option label="Grams" value="GRAMS"/>
    </AD>
    <AD default="%tc,%s,%s,%5.4f" description="String format to use for outputing the measurement value in the file.  Variable ordering is: dateTime, unit, value, error. A consumed string product will use to reset this to a new value." id="measurement.value.format" name="Measurement Value Format" required="true"/>
    <AD default="file:///tmp" description="Identifier referencing the output destination for measurement files.  This will also associate this service with an external node in the monitor view." id="connection.uri" name="Connection uri" required="true"/>
    <AD default="Measurement Trigger File" description="Name to label node in the iRouter Monitor View graphics and other user interface components." id="tracker.monitor.label" name="Component container label" required="false"/>
  </OCD>
  <OCD description="Provides proxy connection and data transfer of measurements from Mettler laboratory weight balances." id="com.verticon.tracker.irouter.mettler.balancefactory" name="Mettler Proxy">
    <AD default="socket://localhost:2345" description="URI to connect to the balance. Example socket://192.168.0.116:2345" id="connection.uri" name="Connection uri" required="true"/>
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD default=".002" description="Value of mesurement error to use for weights reported by the balance as stable" id="stable.weight.error" name="Stable weight measurement error" required="true" type="Double"/>
    <AD default=".01" description="Value of mesurement error to use for weights reported by the balance as unstable." id="unstable.weight.error" name="Unstable weight measurement error" required="true" type="Double"/>
    <AD default="100" description="Number of milliseconds between queries of the balance for new data." id="poll.milliseconds.delay" name="Poll interval milliseconds" required="true" type="Long"/>
    <AD default="" description="Command to request information from the balance. If empty then there will be no queries of the balance. The balance will have to be setup to automatically send data. Example SI%13%%10%" id="request.command" name="Request command" required="false"/>
    <AD default="^S (D|S)((\s*)(.*))\sg$" description="Regular Expression to use to parse return response from the balance." id="response.pattern" name="Response pattern" required="true"/>
    <AD default="5" description="Seconds to wait before retrying to establish a connection to the balance." id="connection.retry.seconds" name="Connection retry interval seconds" required="true" type="Long"/>
    <AD default="mettler.weight" description="Name of the product created by this wire producer and sent to wire consumers." id="producer.weight.measurement.name" name="Producer Scope for the weight measurement" required="true"/>
    <AD default="Mettler Proxy" description="Name to label node in the iRouter Monitor View graphics." id="tracker.monitor.label" name="Component container label" required="false"/>
  </OCD>
