  <OCD description="Provides proxy connection and data transfer of measurements from Ohaus Corporation Series 3000 Weigh indicators" id="com.verticon.tracker.irouter.ohaus" name="Ohaus Proxy">
    <AD default="socket://localhost:2345" description="URI to connect to the balance. Example socket://192.168.0.116:2345" id="connection.uri" name="Connection uri" required="true"/>
    <AD default="one" description="Name of the group of iRouter components that will be wired together. (If no iRouter functionality is required this value is ignored.)" id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD default="5" description="Seconds to wait before retrying to establish a connection to the balance." id="connection.retry.seconds" name="Connection retry interval seconds" required="true" type="Long"/>
    <AD default="50" description="If no data was received from the scale the milliSeconds to wait before checking again." id="response.delay.millis" name="Response delay interval in milliseconds" required="true" type="Long"/>
    <AD default="10" description="Number of times to listen for a scale response before aborting the connection." id="response.retries" name="Response retries before abort" required="true" type="Integer"/>
    <AD cardinality="1" default="ohaus.weight" description="Name of the product created by this wire producer and sent to wire consumers." id="wireadmin.producer.scope" name="Producer Scope" required="true"/>
