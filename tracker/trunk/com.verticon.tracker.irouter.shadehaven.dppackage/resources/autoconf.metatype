  <OCD description="Triggered production of a default animal product from an Animal template document with events timestamped at production time." id="com.verticon.tracker.irouter.premises.animal" name="Premises Animal Producer">
    <AD default="one" description="Name of the group of components that will be wired together." id="tracker.wiring.group.name" name="Wiring group name" required="true"/>
    <AD cardinality="3" default="animal.eid,transaction.state" description="Comma delimited list of the consumed scopes. Required scopes: animal and the transaction ending triggering state. (Use full scope names)." id="wireadmin.consumer.scope" name="Consumable scopes" required="true"/>
