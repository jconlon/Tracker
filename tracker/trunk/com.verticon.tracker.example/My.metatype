<?xml version="1.0" encoding="UTF-8"?>
<metatype:MetaData xmlns:metatype="http://www.osgi.org/xmlns/metatype/v1.1.0">
  <OCD description="Some description goes here" id="mettler.weight" name="Blood Extraction">
    <AD default="" description="Weight of the blood extracted from the animal." id="value" name="Blood Weight" type="Double"/>
    <AD default=".001" description="The precision of the measurement" id="error" name="Measurement Precision" type="Double"/>
    <AD default="kilogram" description="The unit of measurement" id="unit" name="Measurement Unit">
      <Option label="kg" value="kilogram"/>
      <Option label="lb" value="pound"/>
    </AD>
    <AD default="" description="% of Packed Cell Volume" id="pcv" max="100" min="1" name="Percent PCV" required="true" type="Integer"/>
    <Icon resource="platform:/resource/com.verticon.tracker.example/blood.gif" size="16"/>
  </OCD>
  <OCD description="Deworming drug and dosage" id="abbott.deworming" name="Deworming">
    <AD default="" description="Deworming drug" id="abbott.deworming.drug" name="Drug">
      <Option label="Eperinex" value="Eperinex"/>
      <Option label="Fenbendazole" value="Fenbendazole"/>
      <Option label="Ivermectin" value="Ivermectin"/>
      <Option label="Ivomec" value="Ivomec"/>
      <Option label="Safe-Guard" value="Safe-Guard"/>
    </AD>
    <AD default="" description="Worming dosage in ml" id="abbott.deworming.dosage" max="50" min="0" name="Dosage" required="false" type="Double"/>
  </OCD>
</metatype:MetaData>