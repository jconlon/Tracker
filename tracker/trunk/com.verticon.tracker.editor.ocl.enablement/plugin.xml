<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="com.verticon.tracker.ocl.query.viewer.packages">
      <package
            uri="http://www.verticon.com/ns/tracker/0.1.0">
      </package>
   </extension>
   <extension point="org.eclipse.ui.editorActions"> 
   <!-- Premises Editor -->
      <editorContribution 
         id="com.verticon.tracker.ocl.query.editorContribution" 
         targetID="com.verticon.tracker.editor.presentation.TrackerEditorID"> 
        <menu
               label="%_UI_OclMenu_label"
               path="com.verticon.trackerMenuID/additions"
               id="org.eclipse.emf.oclQueryMenuID">
            <separator name="additions"/>
         </menu>
         <action
               label="%_UI_CtxFreeQuery_label"
               class="com.verticon.tracker.ocl.query/com.verticon.tracker.ocl.query.actions.ContextFreeQueryDelegate"
               menubarPath="com.verticon.trackerMenuID/org.eclipse.emf.oclQueryMenuID/additions"
               icon="$nl$/icons/elcl16/oclqryedit_co.gif"
               id="com.verticon.tracker.ocl.query.ContextFreeQueryAction"/>
         <action
               label="%_UI_OclQuery_label"
               class="com.verticon.tracker.ocl.query/com.verticon.tracker.ocl.query.actions.OCLQueryDelegate:http://www.verticon.com/ns/tracker/0.1.0"
               menubarPath="com.verticon.trackerMenuID/org.eclipse.emf.oclQueryMenuID/additions"
               icon="$nl$/icons/elcl16/oclqryedit_co.gif"
               id="com.verticon.tracker.ocl.query.OclQueryAction">
         </action>
         
      </editorContribution>
      <!-- Premises Transaction Editor -->
      <editorContribution
            id="com.verticon.tracker.ocl.query.transactionEditorContribution"
            targetID="com.verticon.tracker.transaction.editor.TrackerEditorID">
         <menu
               label="%_UI_OclMenu_label"
               path="com.verticon.tracker.transaction.TrackerMenuID/additions"
			   id="org.eclipse.emf.oclQueryMenuIDTransactionEditor">
              <separator name="additions"/>
         </menu>
         <action
               label="%_UI_CtxFreeQuery_label"
               class="com.verticon.tracker.ocl.query/com.verticon.tracker.ocl.query.actions.ContextFreeQueryDelegate"
               menubarPath="com.verticon.tracker.transaction.TrackerMenuID/org.eclipse.emf.oclQueryMenuIDTransactionEditor/additions"
               icon="$nl$/icons/elcl16/oclqryedit_co.gif"
               id="com.verticon.tracker.transaction.editor.ocl.query.ContextFreeQueryAction"/>
         <action
               label="%_UI_OclQuery_label"
               class="com.verticon.tracker.ocl.query/com.verticon.tracker.ocl.query.actions.OCLQueryDelegate:http://www.verticon.com/ns/tracker/0.1.0"
               menubarPath="com.verticon.tracker.transaction.TrackerMenuID/org.eclipse.emf.oclQueryMenuIDTransactionEditor/additions"
               icon="$nl$/icons/elcl16/oclqryedit_co.gif"
               id="com.verticon.tracker.transaction.editor.ocl.query.OclQueryAction"/>
      </editorContribution> 
</extension>
</plugin>
