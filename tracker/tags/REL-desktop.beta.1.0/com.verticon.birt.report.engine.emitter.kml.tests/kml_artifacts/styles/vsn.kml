<?xml version="1.0" encoding="utf-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>VSN Styles</name>
    
     <!-- Voyageur placemark icons are green circles and labels are not visible until shown-->
    <Style id="voyageur_highlight_canoe">
      <IconStyle>
        <scale>1</scale>
        <Icon>
          <href>../images/canoe_icon.png</href>
        </Icon>
      </IconStyle>
      <LabelStyle>
        <color>ff00ff00</color>
	<scale>1.2</scale>
      </LabelStyle>
      <LineStyle>
        <color>ff95eaff</color>
        <width>4</width>
      </LineStyle>
    </Style>
    <Style id="voyageur_normal_canoe">
      <IconStyle>
        <scale>1</scale>
        <Icon>
          <href>../images/canoe_icon.png</href>
        </Icon>
      </IconStyle>
      <LabelStyle>
        <scale>0</scale>
      </LabelStyle>
      <LineStyle>
        <color>ff95eaff</color>
        <width>4</width>
      </LineStyle>
    </Style>
    <StyleMap id="Voyageur">
      <Pair>
        <key>normal</key>
        <styleUrl>#voyageur_normal_canoe</styleUrl>
      </Pair>
      <Pair>
        <key>highlight</key>
        <styleUrl>#voyageur_highlight_canoe</styleUrl>
      </Pair>
    </StyleMap>
    
